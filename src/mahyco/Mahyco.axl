  <options>
    <complex name="material" interface="::Types_mahyco::IMaterial" type="Material" minOccurs="1" maxOccurs="unbounded">
      <description> Materiaux </description>
      <simple name="name" type="string">
        <description> Nom du materiau </description>
      </simple>
    </complex>
    <complex name="environment" interface="::Types_mahyco::IEnvironment" type="Environment" minOccurs="1" maxOccurs="unbounded">
      <description> Milieu </description>
      <simple name="name" type="string">
        <description> Nom du milieu </description>
      </simple>
      <simple name="material" type="string" minOccurs="1" maxOccurs="unbounded">
        <description> Nom du materiau </description>
      </simple>
      <service-instance name="eos-model" type="::Eos::IEquationOfState">
        <description> Service d'equation d'état </description>
      </service-instance>
    </complex>
    <service-instance name="cas-model" type="::Cas_test::IInitialisations">
      <description> Service d'initialisation du cas test </description>
    </service-instance>
    <service-instance name="remap-service" type="::Remap::IRemap" default="RemapADI">
      <description> Service de projection </description>
    </service-instance>
    <simple name="cartesian-sort-faces" type="bool" default="true">
      <description> Impose le tri cartésien des faces (valable que si le maillage est lui-même cartésien) </description>
    </simple>
    <simple name="schema-csts" type="bool">
      <description> Schema CSTS </description>
    </simple>
    <simple name="pseudo-centree" type="bool">
      <description> Option pseudo-centrée </description>
    </simple>
    <simple name="longueur-caracteristique" type="string">
      <description> Type de longueur caracteristique </description>
    </simple>
    <simple name="deltat-min" type="real">
      <description>
          Valeur minimale que peut prendre le &lt;em&gt;deltat&lt;/em&gt;
          entre deux itérations.
      </description>

    </simple>
    <simple name="deltat-max" type="real">
      <description>
          Valeur maximale que peut prendre le &lt;em&gt;deltat&lt;/em&gt;
          entre deux itérations. Si le deltat calculé est supérieur
          à cette valeur, il est ramené à cette valeur.
      </description>

    </simple>
    <simple name="deltat-init" type="real">
      <description> Valeur du &lt;em&gt;deltat&lt;/em&gt; au moment du démarrage. </description>
    </simple>
    <simple name="final-time" type="real">
      <name lang="fr">temps--final</name>
      <description>
          Temps auquel on stoppe la simulation. Le code s'arrête dès que
          le temps courant est &lt;strong&gt;strictement&lt;/strong&gt; supérieur à ce temps.
      </description>

    </simple>
    <simple name="cfl" type="real" default="0.3">
      <description> Définition de la CFL </description>
    </simple>
    <simple name="threshold" type="real" default="1.e-10">
      <description> Définition de la valeur limite des petites fractions et autres </description>
    </simple>
    <simple name="sans-lagrange" type="bool" default="false">
      <description> Calcule sans le schema Lagrange </description>
    </simple>
    <simple name="with-newton" type="bool" default="false">
      <description> Calcul de l'energie avec newton </description>
    </simple>
    <simple name="with-projection" type="bool" default="true">
      <description> Calcul avec projection ADI </description>
    </simple>
    <complex name="boundary-condition" interface="::Types_mahyco::IBoundaryCondition" type="BoundaryCondition" minOccurs="0" maxOccurs="unbounded">
      <description> Condition aux limites </description>
      <enumeration name="type" type="::Types_mahyco::BoundaryConditionType">
        <description> Type de condition aux limites </description>
        <enumvalue genvalue="::Types_mahyco::BoundaryConditionType::Vx" name="Vx"/>
        <enumvalue genvalue="::Types_mahyco::BoundaryConditionType::Vy" name="Vy"/>
        <enumvalue genvalue="::Types_mahyco::BoundaryConditionType::Vz" name="Vz"/>
        <enumvalue genvalue="::Types_mahyco::BoundaryConditionType::Unknown" name="unknown"/>
      </enumeration>
      <simple name="surface" type="string">
        <description> Surface sur laquelle s'applique la condition aux limites </description>
      </simple>
      <simple name="value" type="real">
        <description> Valeur de la composante à fixer </description>
      </simple>
    </complex>
    <service-instance name="geometry" type="::Arcane::Numerics::IGeometryMng" default="Euclidian3Geometry"/>
  </options>
</module>
